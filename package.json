{
  "name": "octolit-crm",
  "version": "6.3.0",
  "author": "octolit",
  "private": true,
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false PUBLIC_URL=./ react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false react-scripts build",
    "postbuild": "echo 'Build completed. Consider clearing CDN cache if using one.'",
    "lint": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "electron:dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "GENERATE_SOURCEMAP=false PUBLIC_URL=./ react-scripts build && electron-builder",
    "electron:start": "electron .",
    "electron:pack": "GENERATE_SOURCEMAP=false PUBLIC_URL=./ react-scripts build && electron-builder --dir",
    "electron:dist": "GENERATE_SOURCEMAP=false PUBLIC_URL=./ react-scripts build && electron-builder",
    "electron:dist:win": "GENERATE_SOURCEMAP=false PUBLIC_URL=./ react-scripts build && electron-builder --win",
    "electron:dist:mac": "GENERATE_SOURCEMAP=false PUBLIC_URL=./ react-scripts build && electron-builder --mac",
    "electron:dist:linux": "GENERATE_SOURCEMAP=false PUBLIC_URL=./ react-scripts build && electron-builder --linux",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.octolit.crm",
    "productName": "Octolit CRM",
    "files": [
      "build/**/*",
      "public/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@fullcalendar/core": "6.1.18",
    "@fullcalendar/daygrid": "6.1.18",
    "@fullcalendar/interaction": "6.1.18",
    "@fullcalendar/list": "6.1.18",
    "@fullcalendar/react": "6.1.18",
    "@fullcalendar/timegrid": "6.1.18",
    "@fullcalendar/timeline": "6.1.18",
    "@hookform/resolvers": "^3.1.0",
    "@iconify/react": "^5.0.2",
    "@mui/icons-material": "5.11.16",
    "@mui/lab": "5.0.0-alpha.129",
    "@mui/material": "5.13.0",
    "@mui/system": "5.12.3",
    "@mui/x-date-pickers": "6.3.1",
    "@react-pdf/renderer": "^3.1.9",
    "@reduxjs/toolkit": "1.9.5",
    "apexcharts": "3.40.0",
    "axios": "^1.4.0",
    "axios-queue-js": "^1.1.6",
    "date-fns": "2.30.0",
    "date-fns-tz": "^2.0.0",
    "eslint-config-react-app": "^7.0.1",
    "file-saver": "^2.0.5",
    "formik": "2.2.9",
    "i18next": "22.4.15",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "4.5.0",
    "mui-one-time-password-input": "1.1.0",
    "nprogress": "0.2.0",
    "numeral": "2.0.6",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-apexcharts": "1.4.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-colorful": "^5.6.1",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-helmet-async": "1.3.0",
    "react-hook-form": "^7.44.3",
    "react-hot-toast": "2.4.1",
    "react-i18next": "12.2.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-international-phone": "^2.2.2",
    "react-markdown": "8.0.7",
    "react-qr-code": "^2.0.15",
    "react-quill": "2.0.0",
    "react-redux": "8.0.5",
    "react-router": "6.11.1",
    "react-router-dom": "6.11.1",
